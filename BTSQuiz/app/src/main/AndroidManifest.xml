<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sani.btsquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/btstriviaicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/btstriviaicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".QuizActivity"></activity>
        <activity android:name=".StartingScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".BackgroundSoundService" />
    </application>

</manifest>